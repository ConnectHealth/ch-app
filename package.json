    "react-test-renderer": "^15.4.1",
    "rucksack-css": "^0.9.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.4.2",
    "stylelint-config-standard": "^15.0.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "apollo-client": "^0.7.3",
    "babel-polyfill": "^6.16.0",
