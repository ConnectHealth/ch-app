{
  "name": "ch-app",
  "version": "0.1.0",
  "description": "CH App",
  "main": "index.js",
  "scripts": {
    "build": "webpack --progress --colors",
    "start": "webpack-dev-server --config webpack.config.dev.js --hot --inline --history-api-fallback --output-public-path / --progress --colors --host 0.0.0.0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src",
    "lint:style": "stylelint \"src/**/*.css\"",
    "format": "eslint src --fix",
    "ci": "yarn run lint && yarn run test && yarn run lint:style",
    "serve:coverage": "http-server ./coverage/report -o -p 8082",
    "flow": "flow"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/connecthealh/ch-app"
  },
  "keywords": [],
  "contributors": [
    {
      "name": "Kalman Bekesi",
      "url": "https://github.com/kalmanb"
    }
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-istanbul-loader": "^0.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-flow-react-proptypes": "^0.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.26.1",
    "cssrecipes-defaults": "^0.5.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.8.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-flowtype": "^2.20.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.4.1",
    "flow-bin": "^0.38.0",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.2",
    "json-loader": "^0.5.4",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.0.0",
    "postcss-reporter": "^3.0.0",
    "react-addons-test-utils": "^15.4.1",
