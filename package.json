{
  "name": "ch-app",
  "version": "0.1.0",
  "description": "CH App",
  "main": "index.js",
  "scripts": {
    "build": "webpack --env=prod --progress --profile --colors",
    "start": "webpack-dev-server --env=dev --progress --colors --profile",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src",
    "lint:style": "stylelint \"src/**/*.css\"",
    "format": "eslint src --fix",
    "ci": "yarn run lint && yarn run test && yarn run lint:style",
    "serve:coverage": "http-server ./coverage/report -o -p 8082",
    "flow": "flow",
    "flow:cover": "flow coverage --color",
    "flow:coverage": "flow-coverage-report -i 'src/**/*.js' -x 'src/**/*.spec.js' -t text"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/connecthealh/ch-app"
  },
  "keywords": [],
  "contributors": [
    {
      "name": "Kalman Bekesi",
      "url": "https://github.com/kalmanb"
    }
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-istanbul-loader": "^0.1.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-flow-react-proptypes": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "css-loader": "^0.28.0",
    "cssrecipes-defaults": "^0.5.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "flow-bin": "^0.44.2",
    "fsevents": "^1.1.1",
    "html-webpack-plugin": "^2.28.0",
