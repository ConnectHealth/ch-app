{
  "name": "ch-app",
  "version": "0.1.0",
  "description": "CH App",
  "main": "index.js",
  "scripts": {
    "build": "webpack --progress --colors",
    "start": "webpack-dev-server --config webpack.config.dev.js --hot --inline --history-api-fallback --output-public-path / --progress --colors --host 0.0.0.0",
    "test": "karma start --single-run; exit 0",
    "test:ci": "karma start --single-run",
    "test:watch": "karma start --auto-watch",
    "lint": "eslint src; exit 0",
    "lint:ci": "eslint src",
    "format": "eslint src --fix; exit 0",
    "check": "npm run lint:ci && npm run lint:style:ci && npm run test:ci",
    "serve:coverage": "http-server ./coverage/report -o -p 8082",
    "flow": "flow; exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/connecthealh/ch-app"
  },
